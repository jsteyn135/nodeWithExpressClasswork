<h2 class="center">Subscriber Data for <%= subscriber.name %> </h2>

<table class="table">

    <tbody>
        <tr>
            <td>Name</td>
            <td>
                <%= subscriber.name %>
            </td>
        </tr>
        <tr>
            <td>Email</td>
            <td>
                <%= subscriber.email %>
            </td>
        </tr>
        <tr>
            <td>Zipcode</td>
            <td>
                <%= subscriber.zipCode %>
            </td>
        </tr>
        <tr>
            <td>Num of Courses</td>
            <td><%= subscriber.courses.length %></td>


        </tr>
    </tbody>
</table>